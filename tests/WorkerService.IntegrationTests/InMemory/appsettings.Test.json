{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "WorkerService": "Debug",
      "Microsoft.EntityFrameworkCore": "Information",
      "MassTransit": "Debug"
    }
  },
  "InMemory": {
    "UseDatabase": true,
    "UseMessageBroker": true
  },
  "OpenTelemetry": {
    "ServiceName": "WorkerService-InMemoryTests",
    "Enabled": true,
    "Otlp": {
      "Endpoint": "http://localhost:14250"
    }
  },
  "HealthChecks": {
    "Enabled": true
  },
  "BackgroundServices": {
    "OrderSimulator": {
      "Enabled": false,
      "IntervalMs": 1000,
      "MaxOrders": 5
    },
    "MetricsCollection": {
      "Enabled": true
    },
    "OrderProcessing": {
      "Enabled": false
    }
  },
  "Jwt": {
    "Key": "test-key-with-minimum-256-bits-for-hmac-sha256-algorithm-security",
    "Issuer": "WorkerServiceTest",
    "Audience": "WorkerServiceTestUsers",
    "ExpiryInMinutes": 60
  }
}